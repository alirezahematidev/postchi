{
  "name": "postchi",
  "version": "1.0.0",
  "description": "Convert Postman collections to type-safe fetch API calls",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "postchi": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "ci": "biome ci .",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run",
    "prepare": "husky"
  },
  "keywords": [
    "postman",
    "fetch",
    "typescript",
    "javascript",
    "api",
    "cli",
    "axios",
    "codegen",
    "type-safe"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "picocolors": "^1.0.0",
    "typescript": "^5.3.3",
    "zod": "^3.24.2"
  },
  "optionalDependencies": {
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "release-it": "^17.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1"
  }
}
